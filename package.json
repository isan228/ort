{
  "name": "ort-testing-platform",
  "version": "1.0.0",
  "description": "Платформа для тестирования по ОРТ",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"cyan,magenta\" \"npm run server:dev\" \"npm run client:dev\"",
    "start": "npm run dev",
    "server:dev": "cd server && nodemon index.js",
    "server:start": "cd server && node index.js",
    "client:dev": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": ["ort", "testing", "education"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}

